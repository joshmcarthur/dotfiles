source $HOME/.antigen.zsh

# Load oh-my-zsh library
antigen use oh-my-zsh

# Add bundles
antigen bundle git
antigen bundle autojump
antigen bundle command-not-found
antigen bundle docker-compose

# Local bundles
antigen bundle $HOME/.dotfiles/zsh plugins/git
antigen bundle $HOME/.dotfiles/zsh plugins/java
antigen bundle $HOME/.dotfiles/zsh plugins/go
antigen bundle $HOME/.dotfiles/zsh plugins/docker-compose-shortcuts

# Syntax highlighting
antigen bundle zsh-users/zsh-syntax-highlighting

# Theme
antigen theme $HOME/.dotfiles/zsh themes/sudojosh

# Done
antigen apply
